{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "never"],
        "no-console": ["off", "never"],
        "indent": ["error", 2],
        "import/extensions": ["error", "ignorePackages"]
    }
}
